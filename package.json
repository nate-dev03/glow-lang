{
  "name": "glow-lang",
  "displayName": "Glow Syntax",
  "description": "Syntax highlighting for glow programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "glow",
        "aliases": [
          "Glow",
          "glow"
        ],
        "extensions": [
          ".glow"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "glow",
        "scopeName": "source.glow",
        "path": "./syntaxes/glow.tmLanguage.json"
      }
    ]
  }
}
